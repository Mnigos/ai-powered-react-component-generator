{
	"name": "components-generator-ai",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "biome lint",
		"lint:write": "biome lint --write",
		"format": "biome format",
		"format:write": "biome format --write",
		"check": "biome check",
		"check:write": "biome check --write",
		"typecheck": "pnpm typecheck",
		"docker:postgres": "docker compose up --build -V postgres -d",
		"prisma:generate": "dotenv -e .env.local -- prisma generate",
		"prisma:format": "dotenv -e .env.local -- prisma format",
		"prisma:migrate:dev": "dotenv -e .env.local -- prisma migrate dev",
		"prisma:migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
		"prisma:db:push": "dotenv -e .env.local -- prisma db push",
		"prisma:db:pull": "dotenv -e .env.local -- prisma db pull",
		"postinstall": "dotenv -e .env.local -- prisma generate"
	},
